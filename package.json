{
  "name": "marble",
  "version": "2.0.0",
  "private": true,
  "description": "Two Rock Software website",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "webpack --mode production; vue-cli-service build  /vue/contact-form/src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "file-loader": "^6.2.0",
    "js-cookie": "^2.2.1",
    "vee-validate": "^4.5.0-alpha.1",
    "vue": "^3.0.0",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-the-mask": "^0.11.1",
    "webpack": "4.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^9.5.0",
    "webpack-cli": "^4.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
